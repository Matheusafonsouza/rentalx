  "paths": {
    "/categories": {
      "post": {
        "tags": [
          "Category"
        ],
        "summary": "Create a category",
        "description": "Create a new category",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/definitions/Category"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created"
          },
          "500": {
            "description": "Category already exists"
          }
        }
      },
      "get": {
        "tags": [
          "Category"
        ],
        "summary": "List all categories",
        "description": "List all categories",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/definitions/Specification"
                }
              }
            }
          }
        }
      }
    },
    "/categories/import": {
      "post": {
        "tags": [
          "Category"
        ],
        "summary": "Upload a new category",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "description": "Upload a new category",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "file": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created"
          }
        }
      }
    },
    "/specifications": {
      "post": {
        "tags": [
          "Specifications"
        ],
        "summary": "Create a specification",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "description": "Create a new specification",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/definitions/Specification"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created"
          },
          "500": {
            "description": "Specification already exists"
          }
        }
      }
    },
    "/sessions": {
      "post": {
        "tags": [
          "Sessions"
        ],
        "summary": "Authenticate user",
        "description": "Authenticate with a user",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/definitions/Session"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          },
          "400": {
            "description": "Email or password incorrect"
          }
        }
      }
    },
    "/cars": {
      "post": {
        "tags": [
          "Cars"
        ],
        "summary": "Create a car",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "description": "Create a new car",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/definitions/Car"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Success"
          },
          "400": {
            "description": "Car already exists!"
          }
        }
      }
    }
  },
  "definitions": {
    "Specification": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "description": {
          "type": "string"
        }
      }
    },
    "Category": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "description": {
          "type": "string"
        }
      },
      "example": {
        "name": "CategoryTest",
        "description": "This is a category test"
      }
    },
    "Car": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "brand": {
          "type": "string"
        },
        "category_id": {
          "type": "string"
        },
        "daily_rate": {
          "type": "number"
        },
        "fine_amount": {
          "type": "number"
        },
        "license_plate": {
          "type": "string"
        }
      },
      "example": {
        "name": "car example",
        "description": "This is a car example",
        "brand": "car brand",
        "category_id": "category-id-example",
        "daily_rate": 60,
        "fine_amount": 100,
        "license_plate": "AAA-1234"
      }
    },
    "Session": {
      "type": "object",
      "properties": {
        "email": {
          "type": "string"
        },
        "password": {
          "type": "string"
        }
      },
      "example": {
        "email": "example@email.com",
        "password": "example-password"
      }
    }
  },
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  }
}